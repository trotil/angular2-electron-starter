{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug main process",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/electron/main.ts",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}/dist",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
            "runtimeArgs": [
                "--enable-logging"
            ],
            "env": {},
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/dist/**/*.js"
            ],
            "console": "integratedTerminal"
        }
    ]
}